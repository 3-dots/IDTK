	<configuration-changes>
	   <menu-insert appendTo="DWMenu_Commands">
	        <!-- Show localized menu entry in Dreamweaver menu by specifing 'resid:name'. -->
			<menuitem id="multilingualexample" name="menu inline name" file="commands/multilingualexample/multilingual.htm" resid:name="menu_name" />
		</menu-insert>		
	</configuration-changes>
</macromedia-extension>
